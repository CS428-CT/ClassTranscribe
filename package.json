    "@reduxjs/toolkit": "^1.5.0",
    "@thecodingmachine/redux-toolkit-wrapper": "1.3.0",
    "axios": "^0.21.1",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "husky": "^5.0.9",
    "i18next": "^19.8.4",
    "lint-staged": "^10.5.4",
    "react": "16.13.1",
    "react-i18next": "^11.8.5",
    "react-native": "0.63.4",
    "react-native-appearance": "^0.3.4",
